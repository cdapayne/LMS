<!DOCTYPE html>
<html><head>
<meta charset="utf-8"><title>Accepted Students â€“ Admin</title>
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet">
</head><body class="bg-light">
<div class="container py-5">
  <h1 class="mb-4">Accepted Students</h1>
  <% if (!students.length) { %>
    <div class="alert alert-info">No accepted students.</div>
  <% } else { %>
  <table class="table table-striped align-middle">
    <thead><tr><th>Name</th><th>Username</th><th>Email</th><th>Student ID</th><th>Docs</th></tr></thead>
    <tbody>
      <% students.forEach(s => { %>
        <tr>
          <td><%= s.name %></td>
          <td><%= s.username %></td>
          <td><%= s.email %></td>
          <td><%= s.profile?.studentId || '-' %></td>
          <td><a class="btn btn-sm btn-outline-secondary" href="/admin/students/<%= s.id %>">View</a></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
  <% } %>
  <a href="/admin" class="btn btn-link">Back to dashboard</a>
</div>
</body></html>