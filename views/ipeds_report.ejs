<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8" />
  <title>IPEDS Report â€“ MD Technical School</title>
  <link rel="icon" href="<%= branding?.favicon %>">
  <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" />
</head>
<body>
  <%- include('header', { user: user }) %>
  <div class="container py-4">
    <h1>IPEDS Report</h1>
    <table class="table table-bordered mt-3">
      <tbody>
        <% Object.entries(report).forEach(([k,v]) => { %>
          <tr>
            <th><%= k %></th>
            <td><%= v %></td>
          </tr>
        <% }) %>
      </tbody>
    </table>
    <div class="d-flex gap-2">
      <a class="btn btn-primary" href="/admin/reports/ipeds?format=csv">Download CSV</a>
      <a class="btn btn-secondary" href="/admin/reports/ipeds?format=json">View JSON</a>
      <a class="btn btn-link" href="/admin">Back</a>
    </div>
  </div>
</body>
</html>
